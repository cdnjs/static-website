{
  "name": "cdnjs.dev",
  "version": "1.0.0",
  "description": "The new cdnjs website, using Vue & Nuxt",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "14.18.0"
  },
  "scripts": {
    "dev": "NODE_ENV=dev SITE_HOST=http://localhost:3000/ nuxt",
    "dev:analyze": "NODE_ENV=production SITE_HOST=http://localhost:3000/ nuxt build --analyze",
    "build": "NODE_ENV=production nuxt build",
    "start": "NODE_ENV=production node server/app.js",
    "test": "npm run test:eslint && npm run test:scss",
    "test:fix": "npm run test:eslint:fix && npm run test:scss:fix",
    "test:eslint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test:eslint:fix": "npm run test:eslint -- --fix",
    "test:scss": "sass-lint '!(node_modules|.nuxt|dist)/**/*.scss' --verbose --no-exit --config .sasslintrc",
    "test:scss:fix": "sass-lint-auto-fix '!(node_modules|.nuxt|dist)/**/*.scss'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdnjs/static-website.git"
  },
  "author": "cdnjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cdnjs/static-website/issues"
  },
  "homepage": "https://github.com/cdnjs/static-website#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/sentry": "^5.1.0",
    "@nuxtjs/svg": "^0.1.12",
    "@sentry/node": "^6.10.0",
    "algoliasearch": "^4.10.3",
    "babel-polyfill": "^6.26.0",
    "chunk": "0.0.3",
    "consola": "^2.15.3",
    "copyfiles": "^2.4.1",
    "core-js": "^3.16.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "fontmin": "^0.9.8",
    "glob-to-regexp": "^0.4.1",
    "imagemin": "^8.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "instantsearch.css": "^7.4.5",
    "markdown-it": "^12.1.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "nuxt": "^2.15.7",
    "prismjs": "^1.24.1",
    "semver-sort": "0.0.4",
    "sitemap": "^7.0.0",
    "spdx-license-ids": "^3.0.9",
    "thenby": "^1.3.4",
    "tlite": "^0.1.9",
    "typeface-ubuntu": "^1.1.13",
    "url-parse": "^1.5.3",
    "vue": "^2.6.14",
    "vue-client-only": "^2.1.0",
    "vue-clipboard2": "^0.3.1",
    "vue-gravatar": "^1.4.1",
    "vue-hot-reload-api": "^2.3.4",
    "vue-instantsearch": "^3.8.1",
    "vue-prism-component": "^1.2.0",
    "vue-router": "^3.5.2",
    "vue-select": "^3.12.2"
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "^7.14.9",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@sentry/cli": "^1.68.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.15.0",
    "fibers": "^5.0.0",
    "nodemon": "^2.0.12",
    "sass": "^1.37.0",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.21.2",
    "sass-loader": "^10.2.0",
    "vue-eslint-parser": "^7.10.0"
  }
}
